<launch>
    <group ns="eimo_ground_control">
        <node name="mouse" pkg="eimo_msgs" type="mouse_multi.py.py">
            <env name="ROS_IP" value="192.168.31.236"/>
            <env name="ROS_MASTER_URI" value="http://192.168.31.236:11311/"/>
            <param name="frequency" type='int' value="1"/>
        </node>
    </group>

    <group ns="eimo_remote">
        <machine name="eimo" address="192.168.31.16" env-loader="/home/ubuntu/.bashrc" user="ubuntu" timeout="100" default="true"/>
        <include file="$(find eimo_msgs)/launch/eimo.launch" />
    </group>
</launch>